{
	"name": "nyanya-sudoku",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"nodemon": "nodemon ./nodemon.js",
		"dev": "cp -r ./config.dev.json ./config.temp.json && rm -rf ./.next && cross-env CLIENT_ENV=development next dev -p 23300",
		"build": "next build",
		"devstart": "yarn run build && cross-env CLIENT_ENV=development next start -p 23200",
		"start": "next build && cross-env CLIENT_ENV=production next start -p 23200",
		"export": "next build && next export",
		"docker": "./docker.sh start",
		"log": "./docker.sh logs",
		"build-webworker": "webpack --config ./webworker/webpack.config.js && cp -r ./webworker/dist/webworker-bundle.js ./public/webworker-bundle.js",
		"ts": "npx ts-node ./plugins/test.ts",
		"workder": "tsc ./plugins/sudokuWorker.ts --target esnext -outfile ./public/sudokuWorker.js",
		"workder1": "browserify  ./plugins/sudokuWorker.js > sudokuWorker1.js"
	},
	"dependencies": {
		"@nyanyajs/utils": "^1.0.20",
		"@reduxjs/toolkit": "^1.7.0",
		"@saki-ui/core": "^1.0.3",
		"axios": "^1.6.2",
		"blueimp-md5": "^2.19.0",
		"cross-env": "^7.0.3",
		"i18next": "^21.8.16",
		"moment": "^2.29.4",
		"next": "^12.0.4",
		"next-pwa": "^5.6.0",
		"protobufjs": "^6.11.2",
		"qs": "^6.10.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-i18next": "^11.18.3",
		"react-redux": "^7.2.6",
		"redux": "^4.1.2",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/preset-env": "^7.12.7",
		"@types/blueimp-md5": "^2.18.0",
		"@types/dom-screen-wake-lock": "^1.0.1",
		"@types/node": "^14.14.10",
		"@types/qs": "^6.9.7",
		"@types/react": "^17.0.0",
		"@types/react-redux": "^7.1.16",
		"@types/uuid": "^9.0.7",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^4.0.0",
		"core-js": "^3.8.0",
		"html-webpack-plugin": "^5.5.1",
		"sass": "^1.43.4",
		"ts-loader": "^9.4.2",
		"typescript": "^4.1.2",
		"webpack": "^5.80.0",
		"webpack-cli": "^5.0.2",
		"webpack-dev-server": "^4.13.3"
	}
}
